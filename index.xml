<?xml version="1.0" encoding="UTF-8" ?>	

  <Module>	

    <ModulePrefs title="Hangout to ROS Demo">	
      <Require feature="rpc"/>	
      <Require feature="views"/>	
    </ModulePrefs>	
    <Content type="html">	
      <![CDATA[	
 
       	
        <h1>Hangout to ROS Demo</h1>	
        
        <div id="container"></div>
        <div id="chat" style="text-align:right; border:1px color:#0000FF>My Previous</div>
        <div id="sophia" style="text-align:right; border:1px color:#0000FF>Sophias Previous</div>
        <div id="input" contenteditable="true">Enter Chat Message here</div>
        
        <button id="sendMessage">Send Message</button>	
        
		<script src="//plus.google.com/hangouts/_/api/v1/hangout.js"></script>
        <script type="text/javascript" src="https://rawgit.com/asyncly/EventEmitter2/master/lib/eventemitter2.js"></script>
		<script type="text/javascript" src="https://rawgit.com/RobotWebTools/roslibjs/develop/build/roslib.min.js"></script>	
		<script type="text/javascript" src="https://rawgit.com/tesYolan/rostogapi_final/master/scripts/ros.js"/>
	    
      ]]>	
    </Content>	
  </Module>
